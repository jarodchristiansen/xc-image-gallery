{
    "headers": [
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "DENY"
          },
          {
            "key" : "X-XSS-Protection",
            "value" : "1; mode=block"
          },
          {
            "key" : "Referrer-Policy",
            "value" : "strict-origin"
          },
          {
            "key" : "Permissions-Policy",
            "value" : "geolocation=(self), microphone=()"
          },
          {
            "key": "Content-Security-Policy",
            "value":
              "style-src 'self' *.gstatic.com *.googleapis.com https://*.icomoon.io; script-src 'self' vitals.vercel-insights.com https://xc-image-gallery.vercel.app/ *.xc-image-gallery.vercel.app/; object-src 'none'"
          }
        ]
      }
    ]
  }